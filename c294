a=input().split(' ')
x=[]
for i in a:
    x.append(int(i))
b=sorted(x)
c=str(b[0])+' '+str(b[1])+' '+str(b[2])

if int(b[0])+int(b[1])<=int(b[2]):
    print(c)
    print('No')
elif int(b[0])**2 + int(b[1])**2 < int(b[2])**2:
    print(c)
    print("Obtuse")
elif int(b[0])**2 + int(b[1])**2 == int(b[2])**2:
    print(c)
    print("Right")
elif int(b[0])**2 + int(b[1])**2 > int(b[2])**2:
    print(c)
    print("Acute")
